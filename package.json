{
  "name": "vik-notes",
  "version": "1.0.0",
  "description": "Vikram's Notes",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf _book",
    "docs:build": "npm run clean && npx honkit build ./ _book/notes && rsync -av --exclude='.git' home/ _book/",
    "docs:serve": "npx honkit serve --port 4001",
    "dev": "npm run docs:serve",
    "start": "npm run dev",
    "home:update": "cd home && git pull origin master",
    "deploy": "npm run docs:build && npx firebase-tools deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "notes",
    "documentation",
    "honkit"
  ],
  "author": "Vikram Tiwari",
  "license": "ISC",
  "devDependencies": {
    "firebase-tools": "^15.1.0",
    "gitbook-plugin-tabs": "^0.0.7",
    "honkit": "^6.0.0"
  }
}
